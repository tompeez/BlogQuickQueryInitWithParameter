<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="quickquery-btf">
    <default-activity>QuickQuery</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <input-parameter-definition id="__1">
      <name>tpCityName</name>
      <value>#{pageFlowScope.tpCityName}</value>
      <class>java.lang.String</class>
    </input-parameter-definition>
    <managed-bean id="__2">
      <managed-bean-name>QuickQueryBean</managed-bean-name>
      <managed-bean-class>de.hahn.blog.quickqueryinitwithparameter.view.beans.QuickQueryBean</managed-bean-class>
      <managed-bean-scope>view</managed-bean-scope>
    </managed-bean>
    <view id="QuickQuery">
      <page>/QuickQuery.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
